{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "psychotherapie-seliger-server",
  "version": "../package.json",
  "identifier": "www.psychotherapie-seliger.de",
  "build": {
    "frontendDist": "http://localhost:3000/administration/patients",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "./build.sh"
  },
  "app": {
    "windows": [
      {
        "title": "psychotherapie-seliger",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": ["binary/psychotherapie-seliger"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": "-"
    }
  }
}
