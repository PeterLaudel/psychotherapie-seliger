{
  "name": "psychotherapie-seliger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-pattern=.gitignore --max-warnings=0 .",
    "typecheck": "tsc --incremental false",
    "test": "jest --runInBand",
