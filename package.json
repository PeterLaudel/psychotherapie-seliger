    "db:seed": "ts-node ./tasks/dbSeed.ts",
    "kysely": "kysely",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@googleapis/drive": "^15.0.0",
    "@googleapis/gmail": "^14.0.1",
    "@googleapis/people": "^5.0.1",
    "@googleapis/sheets": "^11.1.0",
    "@material-tailwind/react": "^2.1.10",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@react-pdf/renderer": "^4.3.0",
    "better-sqlite3": "^12.2.0",
    "dayjs": "^1.11.13",
    "google-auth-library": "^10.2.1",
    "kysely": "^0.28.4",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.54.2",
    "qrcode": "^1.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-final-form": "^7.0.0",
    "react-final-form-arrays": "^4.0.0",
    "react-pdf-tailwind": "^2.3.0",
    "schema-dts": "^1.1.5",
    "xmlbuilder2": "^3.1.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@yao-pkg/pkg": "^6.6.0",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "fishery": "^2.3.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "kysely-ctl": "^0.15.1",
    "postcss": "^8",
    "tailwindcss": "^4.1.11",
    "timekeeper": "^2.3.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "xslt3": "^2.7.0"
  },
  "bin": "server.js",
  "pkg": {
    "scripts": [
      "server.js",
      "migrations/*.js"
