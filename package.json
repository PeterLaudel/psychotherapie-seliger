  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path=.gitignore --max-warnings=0 .",
    "typecheck": "tsc --incremental false",
    "test": "jest --runInBand",
    "test:ci": "NODE_ENV=test npm run db:reset && npm run test",
    "ci": "npm run typecheck && npm run lint && npm run test",
    "db:create": "ts-node ./tasks/dbCreate.ts",
    "db:drop": "ts-node ./tasks/dbDrop.ts",
